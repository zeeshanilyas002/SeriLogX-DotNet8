{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": "Information",
    "LogSinks": {
      "EnableFile": true,
      "EnableCloudWatch": true
    },
    "AWS": {
      "Region": "us-east-1",
      "AccessKey": "YOUR_ACCESS_KEY",
      "SecretKey": "YOUR_SECRET_KEY",
      "LogGroup": "LogWiseGroup"
    }
  }
}
